    <Element type="MULTI_SELECT" id="14" concept="PAIN BETTER" question="Does anything make the pain better?" answer="" choices="Change of Position,Walking,Running,Rest"/>
  </Page>
  <Page>
    <Element type="RADIO" id="15" concept="SMOKE" question="Does the patient smoke?" answer="" choices="Yes,No"/> 
    <Element type="RADIO" id="16" concept="FAMILY HEART PROBLEMS" question="Has anyone in the patient's family had heart problems?" answer="" choices="Yes,No"/>
    <Element type="MULTI_SELECT" id="17" concept="OTHER SYMPTOMS CHECKLIST" question="Does the patient have any other associated symptoms? Check all that apply:" answer="" choices="Out of Breath at Rest,Out of Breath with Little Exertion,Out of Breath Lying Flat,Leg Swelling,Feeling Faint,Fainting,Abnormal Heartbeat,Burning Feeling behind Breastbone,Acid Taste in Mouth,Material Coming up from Stomach"/>
