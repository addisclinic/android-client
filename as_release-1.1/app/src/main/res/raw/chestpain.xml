  <Page>
    <Element type="ENTRY" id="7" concept="WALKING DISTANCE" question="How far can the patient walk before needing to rest?" answer=""/>
    <Element type="MULTI_SELECT" id="8" concept="FATIGUE TYPE" question="Does the patient need to stop because of any of the following symptoms?" answer="" choices="Chest Pain,Shortness of Breath,Feeling Faint"/>
    <Element type="RADIO" id="9" concept="PAIN PRESENT" question="Is the patient currently experiencing pain?" answer="" choices="Yes,No"/> 
    <Element type="ENTRY" id="10" concept="PAIN FREQUENCY" question="How often does the patient experience pain?" answer=""/>
