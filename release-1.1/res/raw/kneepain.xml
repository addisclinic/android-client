    <Element type="ENTRY" id="9" concept="WEAKNESS PRESENT" question="Any weakness in affected area?" answer=""/>
    <Element type="ENTRY" id="10" concept="REPETITIVE ACTIVITIES" question="Does the patient conduct any repetitive activities?" answer=""/>
  </Page>
  <Page>
    <Element type="ENTRY" id="11" concept="PAST INJURY" question="Has the patient injured or received treatment for affected area previously?" answer=""/>
    <Element type="ENTRY" id="12" concept="STIFFNESS" question="Does the patient experience stiffness in the morning, if so for how long?" answer=""/>
